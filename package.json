  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node backend/server.js",
    "killa": "kill -9 $(lsof -t -i:3000)",
    "killb": "kill -9 $(lsof -t -i:5000)",
    "killc": "kill -9 $(lsof -t -i:8000)",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run killa\" \"npm run killb\" \"npm run killc\" \"npm run server\" \"npm run client\"",
    "kill": "concurrently \"npm run killa\" \"npm run killb\" \"npm run killc\"",
    "data:import": "node backend/seeder.js",
    "data:destroy": "node backend/seeder.js -d"
  },
